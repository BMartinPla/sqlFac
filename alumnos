CREATE TABLE alumnos(
nro_legajo_a int,
apenomb nchar(30),
nro_doc int,
direccion nchar(50),
email nchar(20),
telefono nchar(20),
cod_doc nchar(5),
sexo nchar(1),
fecnac datetime,
est_civil nchar(10)
);

